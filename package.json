{
  "name": "eexagon",
  "version": "0.1.0",
  "private": true,
  "author": "eknv",
  "license": "UNLICENSED",
  "scripts": {
    "dev": "npm-run-all --parallel back \"web {@}\" --",
    "back": "rimraf dist && npm run back:build:dev && npm-run-all --parallel back:run:dev back:watch",
    "back:build:dev": "cross-env NODE_ENV=development webpack --config webpack.config.server.js",
    "back:build:test": "cross-env NODE_ENV=test webpack --config webpack.config.server.js",
    "back:build": "cross-env NODE_ENV=production webpack --config webpack.config.server.js",
    "back:watch": "cross-env NODE_ENV=development webpack-dev-server --port 2500 --config webpack.config.server.js --hot",
    "back:run:dev": "cross-env NODE_ENV=development node-dev --inspect=localhost:7000 dist/index.js",
    "back:run:test": "cross-env NODE_ENV=test node-dev --inspect=localhost:7000 dist/index.js",
    "back:run": "cross-env NODE_ENV=production node dist/index.js",
    "web": "webpack-dev-server --port 3000 --mode development --config webpack.config.base.js --open --hot --history-api-fallback --env.PLATFORM=local --env.VERSION=stag",
    "web:lint": "eslint --debug web/**/*.ts web/**/*.tsx",
    "web:lint:write": "eslint --debug web/**/*.ts web/**/*.tsx --fix",
    "web:test": "jest",
    "web:test:watch": "jest --watch",
    "web:test:coverage": "jest --coverage --colors",
    "web:prettier": "prettier --write web/**/*.js",
    "mobile": "react-native start",
    "mobile:reset": "react-native start --reset-cache",
    "mobile:clean": "rimraf %LocalAppData%/Temp/metro-cache && rimraf ./android/app/build",
    "android": "react-native run-android",
    "android:log": "react-native log-android",
    "android:bundle": "react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res",
    "android:beta": "fastlane android beta",
    "ios": "react-native run-ios",
    "ios:bundle:test": "ENVFILE=.env.test react-native bundle --platform ios --dev false --entry-file index.js --bundle-output ./ios/main.jsbundle",
    "ios:bundle": "ENVFILE=.env.prod react-native bundle --platform ios --dev false --entry-file index.js --bundle-output ./ios/main.jsbundle",
    "ios:test": "yarn ios:bundle:test && cd ios && fastlane ios beta",
    "ios:prod": "yarn ios:bundle && cd ios && fastlane ios release",
    "mobile:test": "npm run tslint && npm run tsc-test && npm run jest",
    "mobile:debug": "adb reverse tcp:8097 tcp:8097 && react-devtools",
    "mobile:tslint": "tslint --project .",
    "mobile:tsc-test": "tsc --project ./mobile --noEmit",
    "mobile:jest": "jest",
    "mobile:jest-watch": "jest --watchAll",
    "prod:build:web": "webpack --mode production --config webpack.config.prod.js --env.PLATFORM=production --env.VERSION=stag --progress",
    "prod:build:back": "npm run back:build",
    "prod:start": "cd server && yarn start",
    "prisma": "yarn prisma",
    "prisma-import": "prisma import --data prisma/patches/import_2",
    "prisma-deploy": "prisma deploy",
    "prisma-deploy-force": "prisma deploy --force",
    "haul:start": "haul start --platform android",
    "docker:server": "rimraf dist && npm run back:build && docker build -t xebok/eexagon -f Dockerfile_server .",
    "docker:client": "docker build -t xebok/eexagon -f Dockerfile_server ."
  },
  "scriptsComments": {
    "prisma-import": "call it like 'patch=import_1 yarn prisma-import'"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.(js|jsx)": [
      "yarn run web:lint:write",
      "git add"
    ]
  },
